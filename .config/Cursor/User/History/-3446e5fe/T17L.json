{
  "identifier": "window-controls-all",
  "$schema": "../gen/schemas/desktop-schema.json",
  "description": "Allow to use custom controlls on all all windows and create new windows",
  "local": true,
  "windows": [
    "/config/general",
    "main",
    "/config/enterprise",
    "/config/database",
    "/personal/personal/file/personals",
    "/config/tables",
    "/config/new-table",
    "/personal/archive/institutes",
    "/personal/archive/institutes/create",
    "/personal/archive/academic-level",
    "/personal/archive/academic-level/create",
    "/personal/archive/organisms",
    "/personal/archive/organisms/create",
    "/personal/archive/bases",
    "/personal/archive/bases/create",
    "/personal/archive/charges",
    "/personal/archive/charges/create",
    "/personal/archive/physical-signatures",
    "/personal/archive/physical-signatures/create",
    "/personal/personal/search",
    "/personal/personal/id",
    "/personal/archive/profession",
    "/personal/archive/hierarchy",
    "/personal/archive/profession/create",
    "/personal/archive/hierarchy/create",
    "/analysis/management/repein-personal",
    "/analysis/management/repein-personal/personals",
    "/analysis/archive/bands",
    "/analysis/archive/bands/create",
    "/analysis/archive/estate",
    "/analysis/archive/estate/create",
    "/analysis/archive/municipality",
    "/analysis/archive/municipality/create",
    "/analysis/archive/parish",
    "/analysis/archive/parish/create",
    "/analysis/archive/redip",
    "/analysis/archive/redip/create",
    "/analysis/archive/sector",
    "/analysis/archive/sector/create",
    "/analysis/archive/quadrant",
    "/analysis/archive/quadrant/create",
    "/analysis/management/bounding",
    "/analysis/management/update",
    "/analysis/management/query",
    "/analysis/management/rod",
    "/analysis/management/procedure",
    "/analysis/management/query/repeinId"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-minimize",
    "core:window:allow-start-dragging",
    "core:window:allow-maximize",
    "core:window:allow-get-all-windows",
    "core:window:allow-set-enabled",
    "core:window:allow-close",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "core:event:allow-unlisten",
    "core:window:allow-toggle-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-destroy",
    "core:window:allow-create",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-create-webview",
    "store:default",
    "process:allow-restart"
  ]
}
